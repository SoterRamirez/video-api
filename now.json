{
    "name": "educvideo",
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node" }],
    "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
    "env": {
        "DB_USER": "@educvideo-db-user",
        "DB_PASSWORD": "@educvideo-db-password",
        "DB_HOST": "@educvideo-db-host",
        "DB_NAME": "@educvideo-db-name"
    }
}